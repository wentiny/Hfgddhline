<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TextView" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/textView7"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.25"
            android:textColor="#000"
            android:textStyle="bold"
            android:textSize="12dp"
            android:gravity="center"
            android:background="@drawable/tablelayoutbian_little_yellow"
            android:text="名称:" />
        <EditText
            android:id="@+id/editText"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.75"
            android:textSize="13dp"
            android:gravity="center"
            android:background="@drawable/bg_edittext_selector"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear_tv8sp2"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/textView8"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.25"
            android:textColor="#000"
            android:textStyle="bold"
            android:textSize="12dp"
            android:gravity="center"
            android:background="@drawable/tablelayoutbian_little_green"
            android:text="区间（类别）" />
        <Spinner
            android:id="@+id/spinner2"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.75" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear_tv12radio1_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/textView12"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.25"
            android:textColor="#000"
            android:textStyle="bold"
            android:textSize="12dp"
            android:gravity="center|top"
            android:background="@drawable/tablelayoutbian_little_yellow"
            android:text="\n是否江设备点设为默认导航点" />

        <RadioGroup
            android:id="@+id/radio0102"
            android:orientation="horizontal"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.75" >
            <RadioButton
                android:id="@+id/radioButton"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:text="是" />
            <RadioButton
                android:id="@+id/radioButton2"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:text="否" />
        </RadioGroup>
    </LinearLayout>

    <ListView
        android:visibility="gone"
        android:id="@+id/listview2"
        android:layout_width="match_parent"
        android:layout_height="130dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:orientation="horizontal">
        <Button
            android:id="@+id/button3"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.5"
            android:background="@drawable/shape_btn_green"
            android:textColor="#fff"
            android:textStyle="bold"
            android:text="确定" />
        <Button
            android:id="@+id/button4"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.5"
            android:textColor="#000"
            android:background="@drawable/shape_btn_gray"
            android:textStyle="bold"
            android:text="取消" />
    </LinearLayout>

</LinearLayout>
